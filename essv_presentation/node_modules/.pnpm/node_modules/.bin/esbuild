#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules/esbuild/bin/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules/esbuild/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules/esbuild/bin/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules/esbuild/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/esbuild@0.18.16/node_modules:/media/doofnase/QereqolaXebate/CrazyProjects/PauseProcessing/Material/2024-ESSV-Presentation/AwesomeAnranSlides/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
